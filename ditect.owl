    <!-- https://purl.archive.org/purl/ditect/DiTECTOntology#DitectE-nose -->

    <owl:Class rdf:about="https://purl.archive.org/purl/ditect/DiTECTOntology#DitectE-nose">
        <rdfs:subClassOf rdf:resource="https://purl.archive.org/purl/ditect/DiTECTOntology#DitectSensor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.archive.org/purl/ditect/DiTECTOntology/draft#base_value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
